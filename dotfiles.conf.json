{
  "link": {
    "~/.config/nvim/init.vim": "nvimrc.vim",
    "~/.config/mpv": "mpv.conf",
    "~/.config/zsh-goggles/config.toml": "zsh-goggles.toml",
    "~/.curlrc": "curlrc.conf",
    "~/.dotfiles": "",
    "~/.editorconfig": "editorconfig.ini",
    "~/.gdbinit": "gdbinit.conf",
    "~/.ghci": "ghci.hs",
    "~/.gitconfig": "git.conf",
    "~/.gitignore": "gitignore",
    "~/.inputrc": "inputrc",
    "~/.lldbinit": "lldbinit.config",
    "~/.screenrc": "screenrc",
    "~/.vimrc": "vimrc.vim",
    "~/.wgetrc": "wgetrc",
    "~/.zshrc": "profile.zsh"
  },
  "shell": [
    {
      "command": "curl -fLo ~/.vim/autoload/plug.vim --create-dirs https://raw.githubusercontent.com/junegunn/vim-plug/master/plug.vim",
      "description": "Installing vim-plug for vim"
    },
    {
      "command": "curl -fLo ~/.config/nvim/autoload/plug.vim  --create-dirs https://raw.githubusercontent.com/junegunn/vim-plug/master/plug.vim",
      "description": "Installing vim-plug for nvim"
    },
    {
      "command": "curl -fLo ~/.config/mpv/scalers/ravu-r3.hook --create-dirs https://raw.githubusercontent.com/bjin/mpv-prescalers/master/vulkan/compute/ravu-lite-r3.hook",
      "description": "Get RAVU for mpv"
    },
    {
      "command": "curl -fLo ~/.config/mpv/scripts/autocrop.lua --create-dirs https://raw.githubusercontent.com/mpv-player/mpv/master/TOOLS/lua/autocrop.lua",
      "description": "Get autocrop for mpv"
    },
    {
      "command": "curl -fLo ~/.config/mpv/scripts/autodeint.lua --create-dirs https://raw.githubusercontent.com/mpv-player/mpv/master/TOOLS/lua/autodeint.lua",
      "description": "Get autodeint for mpv"
    },
    {
      "command": "mkdir -p ~/.vim/backups ~/.vim/swaps ~/.vim/undo,",
      "description": "Creating directories for vim"
    },
    {
      "command": "test -d ~/.local/bin || mkdir ~/.local/bin",
      "description": "Creating ~/.local/bin"
    },
    {
      "command": "curl https://sh.rustup.rs -sSf | sh -s -- --no-modify-path -y",
      "description": "Install rustup"
    }
  ]
}
